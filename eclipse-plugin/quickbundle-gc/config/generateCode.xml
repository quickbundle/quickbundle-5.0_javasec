	<!-- 个人化配置 -->
	<profileConfig>
		<!-- 关键列正则匹配 -->
		<keyColumnMatch>^title$|^name$</keyColumnMatch>
	</profileConfig>
	<customBundleCode>
		<!-- 可定制的组件列表 -->
		<bundle code="main" name="标准维护" null="true" checked="true">传统Form提交方式的增删改查标准维护，是其它功能的基础。不选则只生成Service层以下的后台代码</bundle>
		<bundle code="ajax" name="Ajax页" null="true" checked="true" depend="main">ExeJS风格的CRUD</bundle>
		<bundle code="importExport" name="导出导入页" null="true" checked="true" depend="main">Excel导出、导入功能</bundle>
		<bundle code="reference" name="参照页" null="true" checked="true" depend="main">作为列表查询的参照页</bundle>
		<bundle code="statistic" name="统计页" null="true" checked="true" depend="main">图表统计、二维表格统计</bundle>
