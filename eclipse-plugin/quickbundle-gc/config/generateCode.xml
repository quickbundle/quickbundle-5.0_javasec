<?xml version="1.0" encoding="UTF-8"?>
<rules rulesVersion="5.0.0-20141211">
	<database pdmPath="" dbProductName="MySQL" driver="com.mysql.jdbc.Driver" url="jdbc:mysql://localhost/rmdemo?jdbcCompliantTruncation=false" userName="root" password="" catalog="" schemaPattern="" tableNamePattern="">
		<!--定义目标表名xml的路径-->
		<tableTos>
			<tableTo xmlName="RM_FUNCTION_NODE.xml">RM_FUNCTION_NODE</tableTo>
		</tableTos>
	</database>
    <codegen baseProjectPath="" toTableNameKeyword="TableName">
       <mvms contextName="JavaEE标准版">
           <mvm>
               <contextName>JavaEE标准版</contextName>
               <codegenConfig>mvm-JavaEE-standard.xml</codegenConfig>
               <framework>jQuery-1.6 + Html + SpringMVC-3.2 + Spring-3.2 + MyBatis-3.2</framework>
               <configProjectClass>org.quickbundle.mda.gc.project.JavaEEStandardProject</configProjectClass>
           </mvm>
       </mvms>
    </codegen>
	<project>
		<javaPackageName>com.helloworld</javaPackageName>
		<javaPackageDir>com/helloworld</javaPackageDir>
		<jspSourcePath>helloworld</jspSourcePath>
		<webAppName>webapp</webAppName>
		<authorName>白小勇</authorName>
	</project>
	<dataType>
		<!--定义数据库类型-->
		<dbTypes>
			<dbType defaultDriver="com.mysql.jdbc.Driver" defaultUrl="jdbc:mysql://localhost/test?useUnicode=true&amp;characterEncoding=UTF-8&amp;jdbcCompliantTruncation=false&amp;autoReconnect=true">MySQL</dbType>
			<dbType defaultDriver="oracle.jdbc.driver.OracleDriver" defaultUrl="jdbc:oracle:thin:@localhost:1521:test">Oracle</dbType>
			<dbType defaultDriver="com.ibm.db2.jcc.DB2Driver" defaultDriver.old="COM.ibm.db2.jdbc.app.DB2Driver" defaultUrl="jdbc:db2://localhost:50000/test">db2</dbType>
			<dbType defaultDriver="net.sourceforge.jtds.jdbc.Driver" defaultUrl="jdbc:jtds:sqlserver://localhost:1433/test;charset=GBK">SQL Server</dbType>
			<dbType defaultDriver="org.h2.Driver" defaultUrl="jdbc:h2:tcp://localhost/~/test">H2</dbType>
			<dbType defaultDriver="com.sybase.jdbc2.jdbc.SybDriver" defaultUrl="jdbc:sybase:Tds:localhost:2638/">Sybase</dbType>
			<dbType defaultDriver="com.informix.jdbc.IfxDriver" defaultUrl="jdbc:informix-sqli://localhost:1533/test:INFORMIXSERVER=myserver">informix</dbType>
			<dbType defaultDriver="org.postgresql.Driver" defaultUrl="jdbc:postgresql://localhost/test">PostgreSQL</dbType>
			<!-- 
			<dbType defaultDriver="sun.jdbc.odbc.JdbcOdbcDriver" defaultUrl="jdbc:odbc:Driver={MicroSoft Access Driver (*.mdb)};DBQ=yourApplicationRealPath/Data/ReportDemo.mdb">access(*)</dbType>
			 -->
			<dbType defaultDriver="undefined driver" defaultUrl="undefined url">other</dbType>
		</dbTypes>
		<!--定义关键字的过滤类型-->
		<filterTypes>
			<filterType>default</filterType>
			<filterType>lowerCase</filterType>
			<filterType>minus</filterType>
			<filterType>specify</filterType>
		</filterTypes>
		<!--定义人性化展现的类型-->
		<humanDisplayTypes>
			<humanDisplayType>default</humanDisplayType>
			<humanDisplayType>rm.dictionary.select</humanDisplayType>
			<humanDisplayType>rm.dictionary.checkbox</humanDisplayType>
			<humanDisplayType>rm.fckEditor</humanDisplayType>
			<humanDisplayType>rm.listReference</humanDisplayType>
			<humanDisplayType>rm.orgReference</humanDisplayType>
			<humanDisplayType>rm.affix</humanDisplayType>
		</humanDisplayTypes>
